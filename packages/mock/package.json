{
  "name": "@smock/mock",
  "version": "0.1.0-beta.1",
  "description": "A useful mock tool.",
  "author": "王世豪<913003120@qq.com>",
  "license": "MIT",
  "keywords": ["mock", "mock-tool", "mock document", "commandLine tools"],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wangshihao111/smock.git"
  },
  "homepage": "https://github.com/wangshihao111/smock/tree/master/packages/mock",
  "bin": {
    "smock": "bin/mock.js"
  },
  "private": false,
  "scripts": {
    "predev": "node build.js --dev",
    "server": "nodemon --exec ts-node src/test-start.ts",
    "prepublish": "tsc",
    "dev": "nuxt",
    "prebuild": "node build.js",
    "build": "nuxt build && tsc",
    "start": "nuxt start",
    "pregenerate": "node build.js",
    "generate": "nuxt generate",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "dev:e2e": "server-test dev :3000 e2e:open",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(html|js|json|vue)\"",
    "test": "start-server-and-test start http-get://localhost:3000 e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": false,
    "printWidth": 100
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "json5": "^2.1.1",
    "lodash": "^4.17.15",
    "commander": "^5.0.0",
    "prettier": "^1.19.1",
    "mockjs": "^1.1.0",
    "axios": "0.19.2",
    "tree-kill": "^1.2.2",
    "lodash-decorators": "6.0.1",
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/google-tag-manager": "^2.3.2",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/toast": "^3.3.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^8.1.0",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.149",
    "@types/mockjs": "^1.0.2",
    "ace-builds": "^1.4.8",
    "firebase": "^7.10.0",
    "graphql": "^14.6.0",
    "graphql-language-service-interface": "^2.3.3",
    "highlight.js": "^9.18.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "nuxt": "^2.12.0",
    "nuxt-i18n": "^6.6.0",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "socket.io-client": "^2.3.0",
    "start-server-and-test": "^1.10.8",
    "typescript": "^3.8.3",
    "v-tooltip": "^2.0.3",
    "vue-virtual-scroll-list": "^1.4.6",
    "vuefire": "^2.2.1",
    "vue-loader": "15.9.1",
    "vuejs-auto-complete": "^0.9.0",
    "vuex-persist": "^2.2.0",
    "yargs-parser": "^18.0.0"
  },
  "files": [
    "dist",
    "lib",
    "bin"
  ]
}
