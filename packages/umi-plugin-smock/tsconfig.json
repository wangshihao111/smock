{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es3",
    "sourceMap": false,
    "outDir": "./",
    "baseUrl": "./",
    "incremental": true,
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["."]
}
